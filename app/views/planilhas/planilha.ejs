            }else{
                var ac = ""
            }
            %>
            <div class="tab-pane <%- ac %>" id="<%- i %>" role="tabpanel" aria-labelledby="<%- i %>-tab">
                <%- dados[i].mes %>
                    <% if(dados[i].contas){%>
                        <div class="container mt-3">
                            <div class="row bg-secondary">
                                <div class="col-sm">id</div>
                                <div class="col-sm">conta</div>
