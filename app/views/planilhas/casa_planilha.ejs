            var valor = "R$ "+dados[i].valor.toFixed(2).replace(".",",")
            var valorPagar = 0.00
            var contas = dados[i].desc
            var st = false;
            var id = dados[i].id;
            var colorbg = "";
            (i == 0)?active = "active show":active = ""
    %>      
        <div class="tab-pane fade <%= active %>" id="<%= id %>" role="tabpanel" aria-labelledby="<%= id %>-tab">
            <%- include casa_planilha_table.ejs %>
        </div>
    <%
    
        }
    %>
</div>
