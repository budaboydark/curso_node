 <div class="container">
    <!-- Nav tabs -->
    <ul class="nav nav-tabs" id="myTab" role="tablist">
        <% 
        var active2 = '';
        for(var tab=0; tab < dados.length;tab++){   
        %>
            <% var id_tab = dados[tab].id %>
            <% var mes_tab = dados[tab].mes %>
            <% 
            (tab == 0)?active2 = 'active':active2= ''
            %>
            <li class="nav-item">
                    <a class="nav-link <%= active%>" id="<%= id_tab %>-tab" data-toggle="tab" href="#<%= id_tab%>" role="tab" aria-controls="1" aria-selected="true"><%= mes_tab%></a>
            </li>
        <% } %>
    </ul>
</div>
<div class="tab-content" id="nav-tabContent">
