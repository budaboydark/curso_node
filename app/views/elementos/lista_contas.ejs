<div class="container mt-3">
    <div class="row bg-secondary">
        <div class="col-sm">Id</div>
        <div class="col-sm">Conta</div>
        <div class="col-sm">Qtd_parcela</div>
        <div class="col-sm">Valor</div>
        <div class="col-sm">Tipo</div>
    </div>
    <% for(var i=0; i < contas.length;i++){ 
        var id = contas[i]["id"]
        var tipo = contas[i]["tipo"]
        
        %>
        <div class="row">
            <div class="col-sm">
                <%= contas[i]["id"]%>
            </div>
            <div class="col-sm">
                <%= contas[i]["conta"]%>
            </div>
            <div class="col-sm">
                <%= contas[i]["qtd_parcelas"]%>
            </div>
            <div class="col-sm">
                <%= (contas[i]["qtd_parcelas"] * contas[i]["valor"]) %>
            </div>
            <div class="col-sm">
                <a href="/contas/<%- id %>/<%- tipo %>">
                    <%= tipo%>
                </a>
            </div>
        </div>
        <%  } %>
</div>